<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="PageBackground" Color="#0A1220" />
    <SolidColorBrush x:Key="PanelBackground" Color="#141E32" />
    <SolidColorBrush x:Key="PanelBorder" Color="#2D4265" />
    <SolidColorBrush x:Key="TextPrimary" Color="#F8FAFF" />
    <SolidColorBrush x:Key="TextMuted" Color="#B9C8E6" />
    <SolidColorBrush x:Key="InputBackground" Color="#111A2C" />
    <SolidColorBrush x:Key="InputBorder" Color="#4D74A5" />
    <SolidColorBrush x:Key="SidebarBackground" Color="#10192B" />
    <SolidColorBrush x:Key="OverlayBackground" Color="#0F1A2A" />
    <SolidColorBrush x:Key="ButtonBackground" Color="#264A76" />
    <SolidColorBrush x:Key="ButtonBorder" Color="#4370A6" />
    <SolidColorBrush x:Key="ButtonForeground" Color="#FFFFFF" />
    <SolidColorBrush x:Key="NavHoverBackground" Color="#1F3555" />
    <SolidColorBrush x:Key="NavSelectedBackground" Color="#2E507C" />
    <SolidColorBrush x:Key="WarningText" Color="#FFEEC9A6" />
    <SolidColorBrush x:Key="Accent" Color="#39C3FF" />
    <SolidColorBrush x:Key="AccentStrong" Color="#1DA7E5" />
    <SolidColorBrush x:Key="AccentText" Color="#041321" />

    <SolidColorBrush x:Key="VmChipBaseBackground" Color="#36506D" />
    <SolidColorBrush x:Key="VmChipBaseBorder" Color="#5B84AE" />
    <SolidColorBrush x:Key="VmChipRunningBackground" Color="#1F8B4A" />
    <SolidColorBrush x:Key="VmChipRunningBorder" Color="#49D17A" />
    <SolidColorBrush x:Key="VmChipOffBackground" Color="#5A2F42" />
    <SolidColorBrush x:Key="VmChipOffBorder" Color="#B66D8D" />
    <SolidColorBrush x:Key="VmChipSelectedBorder" Color="#F7C948" />

    <SolidColorBrush x:Key="ContextMenuBackgroundBrush" Color="#111A2C" />
    <SolidColorBrush x:Key="ContextMenuBorderBrush" Color="#4D74A5" />
    <SolidColorBrush x:Key="ContextMenuForegroundBrush" Color="#F8FAFF" />
    <SolidColorBrush x:Key="ContextMenuHoverBrush" Color="#1F3555" />

    <SolidColorBrush x:Key="ToolTipBackgroundBrush" Color="#1A2740" />
    <SolidColorBrush x:Key="ToolTipBorderBrush" Color="#4D74A5" />
    <SolidColorBrush x:Key="ToolTipForegroundBrush" Color="#F8FAFF" />

    <SolidColorBrush x:Key="SystemWindowBrush" Color="#111A2C" />
    <SolidColorBrush x:Key="SystemWindowTextBrush" Color="#FFFFFF" />
    <SolidColorBrush x:Key="SystemControlBrush" Color="#111A2C" />
    <SolidColorBrush x:Key="SystemControlTextBrush" Color="#FFFFFF" />
    <SolidColorBrush x:Key="SystemMenuBrush" Color="#111A2C" />
    <SolidColorBrush x:Key="SystemMenuTextBrush" Color="#FFFFFF" />
    <SolidColorBrush x:Key="SystemHighlightBrush" Color="#39C3FF" />
    <SolidColorBrush x:Key="SystemHighlightTextBrush" Color="#041321" />

    <SolidColorBrush x:Key="{x:Static SystemColors.WindowBrushKey}" Color="#111A2C" />
    <SolidColorBrush x:Key="{x:Static SystemColors.WindowTextBrushKey}" Color="#FFFFFF" />
    <SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrushKey}" Color="#39C3FF" />
    <SolidColorBrush x:Key="{x:Static SystemColors.HighlightTextBrushKey}" Color="#041321" />
    <SolidColorBrush x:Key="{x:Static SystemColors.ControlBrushKey}" Color="#111A2C" />
    <SolidColorBrush x:Key="{x:Static SystemColors.ControlTextBrushKey}" Color="#FFFFFF" />
    <SolidColorBrush x:Key="{x:Static SystemColors.MenuBrushKey}" Color="#111A2C" />
    <SolidColorBrush x:Key="{x:Static SystemColors.MenuTextBrushKey}" Color="#FFFFFF" />
</ResourceDictionary>
